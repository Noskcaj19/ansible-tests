# Do not edit directly. Managed by Ansible
[libdefaults]
 default_realm = {{ base_server_vas_realm }}
 ticket_lifetime = 10h
 forwardable = true
 default_keytab_name = /etc/opt/quest/vas/host.keytab
 default_etypes = aes256-cts-hmac-sha1-96 aes128-cts-hmac-sha1-96 arcfour-hmac-md5

[domain_realm]
 {{ ansible_facts['fqdn'] }} = {{ base_server_vas_realm }}
 #.{{ ansible_facts['domain'] }} = {{ base_server_vas_realm }}


[libvas]
 use-server-referrals = true
 use-dns-srv = true

[vasd]
 workstation-mode = true
